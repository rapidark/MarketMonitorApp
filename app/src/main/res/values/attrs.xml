<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FGORecyclerView">
        <attr name="recycleviewDispatch" format="boolean"/>
        <attr name="recyclerviewClipToPadding" format="boolean" />
        <attr name="recyclerviewPadding" format="dimension" />
        <attr name="recyclerviewPaddingTop" format="dimension" />
        <attr name="recyclerviewPaddingBottom" format="dimension" />
        <attr name="recyclerviewPaddingLeft" format="dimension" />
        <attr name="recyclerviewPaddingRight" format="dimension" />
        <attr name="recyclerviewEmptyView" format="reference" />
        <attr name="recyclerviewScrollbars" format="enum">
            <enum name="none" value="0" />
            <enum name="vertical" value="1" />
            <enum name="horizontal" value="2" />
        </attr>
        <attr name="recyclerviewDefaultSwipeColor" format="reference"></attr>
    </declare-styleable>

    <declare-styleable name="GestureLockViewGroup">
        <attr name="color_no_finger_inner_circle" format="color" />
        <attr name="color_no_finger_outer_circle" format="color" />
        <attr name="color_finger_on" format="color" />
        <attr name="color_finger_up" format="color" />
        <attr name="count" format="integer" />
        <attr name="tryTimes" format="integer" />
    </declare-styleable>

    <declare-styleable name="GestureLockPreviewGroup">
        <attr name="color_no_checked" format="color" />
        <attr name="color_checked" format="color" />
        <attr name="count_preview" format="integer" />
    </declare-styleable>
</resources>